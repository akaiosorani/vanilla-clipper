{
  "name": "vanilla-clipper",
  "version": "0.0.0",
  "repository": "github:yarnaimo/vanilla-clipper",
  "license": "MIT",
  "author": "yamaimo <yarnaimo@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "debug": "node --inspect -r ts-node/register",
    "dev": "yarn ts-node src/index.ts",
    "prepare": "yarn build",
    "start": "NODE_ENV=production node -r tsconfig-paths/register dist/index.js",
    "test": "NODE_ENV=test jest -w1",
    "ts-node": "ts-node -r tsconfig-paths/register"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@yarnaimo/rain": "^0.1.2",
    "class-validator": "^0.9.1",
    "filenamify-url": "^1.0.0",
    "got": "^9.3.2",
    "luxon": "^1.8.2",
    "puppeteer": "^1.10.0",
    "signale": "^1.3.0",
    "tldjs": "^2.3.1",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/expect-puppeteer": "^3.3.0",
    "@types/got": "^9.2.1",
    "@types/jest-environment-puppeteer": "^2.2.1",
    "@types/luxon": "^1.4.1",
    "@types/micro": "^7.3.3",
    "@types/puppeteer": "^1.10.0",
    "@types/signale": "^1.2.0",
    "@types/tldjs": "^2.3.0",
    "@types/yargs": "^12.0.1",
    "@yarnaimo/tss": "^0.0.1",
    "jest-puppeteer": "^3.5.2",
    "micro": "^9.3.3",
    "serve-handler": "^5.0.7"
  },
  "lint-staged": {
    "./package.json": [
      "sort-package-json",
      "git add"
    ],
    "*.{js,jsx,ts,tsx,css,scss,vue,gql,md}": [
      "prettier --write --ignore-path .gitignore",
      "git add"
    ]
  }
}
